{
  "name": "Enrollment",
  "plural": "enrollments",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "created": {
      "type": "date",
      "default": "$now"
    },
    "feedback": {
      "type": [
        "object"
      ],
      "default": []
    },
    "progress": {
      "type": "number",
      "default": 0
    },
    "finished": {
      "type": "boolean",
      "default": "false"
    },
    "returned": {
      "type": "boolean",
      "default": "false"
    },
    "lastActivity": {
      "type": "date",
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "course": {
      "type": "belongsTo",
      "model": "Course",
      "foreignKey": "courseId"
    },
    "student": {
      "type": "belongsTo",
      "model": "Account",
      "foreignKey": ""
    },
    "flags": {
      "type": "hasMany",
      "model": "Flag",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
